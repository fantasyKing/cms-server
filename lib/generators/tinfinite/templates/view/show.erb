<% content_for(:page_header) do %>
  <%= "Database" %>
<% end %>
<% content_for :panel_heading do %>Details App: app_info<% end %>

<h3>app_info</h3>
<div class="block">
  <div class="block-header">
    app_info #<%= @app_info.app_name %>
  </div>
  <div class="block-content">
    <div class="table-responsive">
      <table class="table table-striped table-bordered">
        <tbody>
          <% attrs = @app_info.attributes.except("_id").keys %>
          <% attrs.each do |a| %>
          <tr>
            <td><%= a.humanize %></td>
            <td><%= @app_info.attributes[a].to_s %></td>
          </tr>
          <% end %>

        </tbody>
      </table>
    </div>
  </div>
</div>
